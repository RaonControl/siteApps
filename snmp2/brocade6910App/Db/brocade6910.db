# Connector Type
record(fanout, "${L}:${D}:ReadType")
{
  field(LNK1, "${L}:${D}:FAN1")
  field(LNK2, "${L}:${D}:FAN2")
  field(PINI, "YES")
  field(VAL, "1")
}

