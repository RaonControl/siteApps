record(snmp, "${USER}:MainPower") {
  field(DESC, "WIENER Main Power Switch") # ON: 1, OFF: 0
  field(DTYP, "SNMP Read")
  field(SCAN, "1 second")
  field(HOST, "10.1.5.123")
  field(COMM, "wiener")
  field(OIDS, "${W}sysMainSwitch.0")
}

record(snmp, "${USER}:FanairTemp") {
  field(DESC, "WIENER Fan Temperature") # Read only
  field(DTYP, "SNMP Read")
  field(SCAN, "1 second")
  field(HOST, "10.1.5.123")
  field(COMM, "wiener")
  field(OIDS, "${W}fanAirTemperature.0")
}

record(snmp, "${USER}:FanNominalSpeed") {
  field(DESC, "WIENER Fan rotation speed") # MIN: 1200, MAX: 3600
  field(DTYP, "SNMP Read")
  field(SCAN, "1 second")
  field(HOST, "10.1.5.123")
  field(COMM, "wiener")
  field(OIDS, "${W}fanNominalSpeed.0")
}

record(snmp, "${USER}:CrateStatus") {
  field(DESC, "WIENER Crate Status") 
  field(DTYP, "SNMP Read")
  field(SCAN, "1 second")
  field(HOST, "10.1.5.123")
  field(COMM, "wiener")
  field(OIDS, "${W}sysStatus.0")
}
